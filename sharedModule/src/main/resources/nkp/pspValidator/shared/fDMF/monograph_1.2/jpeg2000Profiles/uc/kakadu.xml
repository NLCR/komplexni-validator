                    <regexp>Kakadu Core Error:((?!Kakadu Core Error).)*</regexp>
                </allNonempty>
            </textDataExtraction>
            <rules>
                <mustNotExist/>
            </rules>
        </validation>

    </fromText>
</jpeg2000profile>
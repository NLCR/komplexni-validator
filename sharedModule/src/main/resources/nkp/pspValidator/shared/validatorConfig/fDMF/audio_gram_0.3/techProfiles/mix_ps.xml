<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<metadataProfile
        xmlns="http://www.nkp.cz/pspValidator/2.3/metadataProfile"
        name="MIX PS (primarní sken)"
        validatorVersion="2.3"
        dmf="audio_gram_0.3"

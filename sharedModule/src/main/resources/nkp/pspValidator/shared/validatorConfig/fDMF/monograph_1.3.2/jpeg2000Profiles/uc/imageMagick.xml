<jpeg2000profile
        xmlns="http://www.nkp.cz/pspValidator/2.0/jpeg2000profile"
        validatorVersion="2.0"
        dmf="monograph_1.3.1"
>
    <fromText>

        <validation name="no_errors">
            <textDataExtraction resultType="STRING_LIST">
                <allNonempty>
                    <regexp>(?m)^Error.*$</regexp>
                    <regexp>(?ms)identify:((?!identify).)*</regexp>
                </allNonempty>
            </textDataExtraction>
            <rules>
                <mustNotExist/>
            </rules>
        </validation>

    </fromText>
</jpeg2000profile>
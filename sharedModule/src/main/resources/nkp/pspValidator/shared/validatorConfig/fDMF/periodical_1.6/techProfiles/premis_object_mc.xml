<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<validationTemplate
        xmlns="http://www.nkp.cz/pspValidator/1.4/metadataProfile"
        name="PREMIS Object pro MC (archivní kopie)"
        validatorVersion="1.4"
        dmf="periodical_1.6"
>

    <info>
        <text>
            Profil pro validaci PREMIS Object záznamu archivní kopie (MC) stránky v sekundárním MODSu pro DMF
            Periodika-1.6.
        </text>
        <url description="popis formátu PREMIS">http://www.loc.gov/standards/premis/</url>
        <url description="XSD pro PREMIS 2.2">http://www.loc.gov/standards/premis/v2/premis-v2-2.xsd</url>
    </info>

    <namespaces>
        <namespace prefix="mets">http://www.loc.gov/METS/</namespace>
        <namespace prefix="premis">info:lc/xmlns/premis-v2</namespace>
        <namespace prefix="xsi">http://www.w3.org/2001/XMLSchema-instance</namespace>
    </namespaces>

    <rootElement name="mets:techMD">
        <expectedAttributes>
            <attribute name="ID">
                <expectedContent>
                    <value>OBJ_002</value>
                </expectedContent>
            </attribute>
        </expectedAttributes>

        <expectedElements>
            <element name="mets:mdWrap">
                <expectedAttributes>
                    <attribute name="MDTYPE">
                        <expectedContent>
                            <value>PREMIS</value>
                        </expectedContent>
                    </attribute>
                </expectedAttributes>

                <expectedElements>
                    <element name="mets:xmlData">
                        <expectedElements>
                            <element name="premis:object" mandatory="true" repeatable="false">
                                <expectedAttributes>
                                    <attribute name="xsi:type">
                                        <expectedContent>
                                            <oneOf>
                                                <!--<value>premis:file</value>-->
                                                <value>file</value>
                                                <value>representation</value>
                                                <value>bitstream</value>
                                            </oneOf>
                                        </expectedContent>
                                    </attribute>
                                </expectedAttributes>
                                <expectedElements>
                                    <element name="premis:objectIdentifier" mandatory="true" repeatable="true">
                                        <expectedElements>
                                            <element name="premis:objectIdentifierType" mandatory="true"
                                                     repeatable="false">
                                                <expectedContent/>
                                            </element>

                                            <element name="premis:objectIdentifierValue" mandatory="true"
                                                     repeatable="false">
                                                <expectedContent/>
                                            </element>
                                        </expectedElements>
                                    </element>

                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 57. Nechavam defenzivne nepovinne-->
                                    <element name="premis:preservationLevel" mandatory="false" repeatable="true">
                                        <expectedElements>
                                            <element name="premis:preservationLevelValue" mandatory="true"
                                                     repeatable="false">
                                                <expectedContent>
                                                    <value>preservation</value>
                                                </expectedContent>
                                            </element>

                                            <element name="premis:preservationLevelDateAssigned" mandatory="false"
                                                     repeatable="false">
                                                <expectedContent>
                                                    <regexp>[0-9]{4}-[0-9]{2}-[0-9]{2}</regexp>
                                                </expectedContent>
                                            </element>
                                        </expectedElements>
                                    </element>

                                    <element name="premis:objectCharacteristics" mandatory="true" repeatable="true">
                                        <expectedElements>
                                            <element name="premis:compositionLevel" mandatory="true" repeatable="false">
                                                <expectedContent/>
                                            </element>

                                            <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 58. Nechavam defenzivne nepovinne-->
                                            <element name="premis:fixity" mandatory="false" repeatable="true">
                                                <expectedElements>
                                                    <element name="premis:messageDigestAlgorithm" mandatory="true"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>

                                                    <element name="premis:messageDigest" mandatory="true"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>
                                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 58. Nechavam defenzivne nepovinne-->
                                                    <element name="premis:messageDigestOriginator" mandatory="false"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>
                                                </expectedElements>
                                            </element>

                                            <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 58. Nechavam defenzivne nepovinne-->
                                            <element name="premis:size" mandatory="false" repeatable="false">
                                                <expectedContent>
                                                    <regexp>[0-9]+</regexp>
                                                </expectedContent>
                                            </element>

                                            <element name="premis:format" mandatory="false" repeatable="true">
                                                <expectedElements>
                                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 58. Nechavam defenzivne nepovinne-->
                                                    <element name="premis:formatDesignation" mandatory="false"
                                                             repeatable="false">
                                                        <expectedElements>
                                                            <element name="premis:formatName" mandatory="true"
                                                                     repeatable="false">
                                                                <expectedContent/>
                                                            </element>

                                                            <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 58. Nechavam defenzivne nepovinne-->
                                                            <element name="premis:formatVersion" mandatory="false"
                                                                     repeatable="false">
                                                                <expectedContent/>
                                                            </element>
                                                        </expectedElements>
                                                    </element>

                                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 58. Nechavam defenzivne nepovinne-->
                                                    <element name="premis:formatRegistry" mandatory="false"
                                                             repeatable="false">
                                                        <expectedElements>
                                                            <element name="premis:formatRegistryName" mandatory="true"
                                                                     repeatable="false">
                                                                <expectedContent/>
                                                            </element>

                                                            <element name="premis:formatRegistryKey" mandatory="true"
                                                                     repeatable="false">
                                                                <expectedContent/>
                                                            </element>
                                                        </expectedElements>
                                                    </element>
                                                </expectedElements>
                                            </element>

                                            <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 59. Nechavam defenzivne nepovinne-->
                                            <element name="premis:creatingApplication" mandatory="false"
                                                     repeatable="true">
                                                <expectedElements>
                                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-1, viz DMF Periodika 1.6: s. 59. Nechavam defenzivne nepovinne-->
                                                    <element name="premis:creatingApplicationName" mandatory="false"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>

                                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-1, viz DMF Periodika 1.6: s. 59. Nechavam defenzivne nepovinne-->
                                                    <element name="premis:creatingApplicationVersion" mandatory="false"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>

                                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-1, viz DMF Periodika 1.6: s. 59. Nechavam defenzivne nepovinne-->
                                                    <element name="premis:dateCreatedByApplication" mandatory="false"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>
                                                </expectedElements>
                                            </element>
                                        </expectedElements>
                                    </element>

                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-1, viz DMF Periodika 1.6: s. 59. Nechavam defenzivne nepovinne-->
                                    <element name="premis:originalName" mandatory="false" repeatable="false">
                                        <expectedContent/>
                                    </element>

                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 59. Nechavam defenzivne nepovinne-->
                                    <element name="premis:relationship" mandatory="false" repeatable="true">
                                        <expectedElements>
                                            <element name="premis:relationshipType" mandatory="true" repeatable="false">
                                                <expectedContent>derivation</expectedContent>
                                            </element>

                                            <element name="premis:relationshipSubType" mandatory="true"
                                                     repeatable="false">
                                                <expectedContent/>
                                            </element>

                                            <element name="premis:relatedObjectIdentification" mandatory="true"
                                                     repeatable="true">
                                                <expectedElements>
                                                    <element name="premis:relatedObjectIdentifierType" mandatory="true"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>

                                                    <element name="premis:relatedObjectIdentifierValue" mandatory="true"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>
                                                </expectedElements>
                                            </element>

                                            <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 60. Nechavam defenzivne nepovinne-->
                                            <element name="premis:relatedEventIdentification" mandatory="false"
                                                     repeatable="true">
                                                <expectedElements>
                                                    <element name="premis:relatedEventIdentifierType" mandatory="true"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>

                                                    <element name="premis:relatedEventIdentifierValue" mandatory="true"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>

                                                    <element name="premis:relatedEventSequence" mandatory="false"
                                                             repeatable="false">
                                                        <expectedContent/>
                                                    </element>
                                                </expectedElements>
                                            </element>
                                        </expectedElements>

                                    </element>

                                    <!--Neni jasna povinnost - konflikt povinnost=M a vyskyt=0-n, viz DMF Periodika 1.6: s. 59. Nechavam defenzivne nepovinne-->
                                    <element name="premis:linkingEventIdentifier" mandatory="false"
                                             repeatable="true">
                                        <expectedElements>
                                            <element name="premis:linkingEventIdentifierType" mandatory="true"
                                                     repeatable="true">
                                                <expectedContent/>
                                            </element>

                                            <element name="premis:linkingEventIdentifierValue" mandatory="true"
                                                     repeatable="true">
                                                <expectedContent/>
                                            </element>
                                        </expectedElements>
                                    </element>
                                </expectedElements>
                            </element>
                        </expectedElements>
                    </element>
                </expectedElements>
            </element>
        </expectedElements>
    </rootElement>

</validationTemplate>
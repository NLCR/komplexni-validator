<jpeg2000profile
        xmlns="http://www.nkp.cz/pspValidator/2.1/jpeg2000profile"
        validatorVersion="2.1"
        dmf="periodical_1.6"
>
    <fromText>

        <validation name="no_errors">
            <textDataExtraction resultType="STRING_LIST">
                <allNonempty>
                    <regexp>(?ms)Kakadu Core Error:((?!Kakadu Core Error).)*</regexp>
                </allNonempty>
            </textDataExtraction>
            <rules>
                <mustNotExist/>
            </rules>
        </validation>

    </fromText>
</jpeg2000profile>
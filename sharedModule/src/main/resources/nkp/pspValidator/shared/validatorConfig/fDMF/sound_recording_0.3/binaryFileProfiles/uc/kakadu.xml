<binaryFileProfile
        xmlns="http://www.nkp.cz/pspValidator/2.2/binaryFileProfile"
        validatorVersion="2.2"
        dmf="sound_recording_0.3"
>
    <fromText>

        <validation name="no_errors">
            <textDataExtraction resultType="STRING_LIST">
                <allNonempty>
                    <regexp>(?ms)Kakadu Core Error:((?!Kakadu Core Error).)*</regexp>
                </allNonempty>
            </textDataExtraction>
            <rules>
                <mustNotExist/>
            </rules>
        </validation>

    </fromText>
</binaryFileProfile>
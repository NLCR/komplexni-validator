<jpeg2000profile
        xmlns="http://www.nkp.cz/pspValidator/2.1/jpeg2000profile"
        validatorVersion="2.1"
        dmf="periodical_1.4"
>
<fromText>

        <validation name="no_errors">
            <textDataExtraction resultType="STRING_LIST">
                <allNonempty>
                    <regexp>(?m)^Error.*$</regexp>
                    <regexp>(?ms)identify:((?!identify).)*</regexp>
                </allNonempty>
            </textDataExtraction>
            <rules>
                <mustNotExist/>
            </rules>
        </validation>

    </fromText>
</jpeg2000profile>
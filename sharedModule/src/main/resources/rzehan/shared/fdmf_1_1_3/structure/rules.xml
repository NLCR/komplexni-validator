<rules>

    <description>
        Zde jsou definovány validační pravidla. K samotným validacím se používají validační funkce, jejichž parametry
        jsou definované proměnné, vzory a konstanty.
        Pravidla je možné dynamicky doplňovat/upravovat, ale je vždy nutné používat existující proměnné a vzory.
    </description>


    <section name="PSP ID" description="Identifikátor PSP">


        <!--psp id-->

        <rule name="PSP_ID_DERIVED_FROM_TITLE_ID" level="warning">
            <description>PSP-ID musí být odvozen od některého z povolených identifikátorů.</description>
            <!--TODO-->
        </rule>

    </section>


    <section name="Struktura souborů" description="Struktura PSP balicku na urovni soboru">


        <!--psp dir-->

        <rule name="PSP_DIR_MATCHES_PSP_ID" level="warning">
            <description>Název kořenového adresáře musí být schodný s PSP-ID.</description>
            <validationFunction name="CHECK_FILENAME_MATCHES_STRING">
                <params>
                    <param name="file">
                        <variable name="PSP_DIR"/>
                    </param>
                    <param name="string">
                        <variable name="PSP_ID"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--právě jeden každý top-level soubor-->

        <rule name="SINGLE_INFO_FILE_EXISTS" level="severe">
            <description>Musí existovat nanejvýše jeden soubor INFO.</description>
            <validationFunction name="CHECK_LIST_EXACT_SIZE">
                <params>
                    <param name="list">
                        <variable name="INFO_FILES"/>
                    </param>
                    <param name="size">1</param>
                </params>
            </validationFunction>
        </rule>

        <rule name="SINGLE_MAIN_METS_FILE_EXISTS" level="severe">
            <description>Musí existovat nanejvýše jeden soubor HLAVNI-METS.</description>
            <validationFunction name="CHECK_LIST_EXACT_SIZE">
                <params>
                    <param name="list">
                        <variable name="MAIN_METS_FILES"/>
                    </param>
                    <param name="size">1</param>
                </params>
            </validationFunction>
        </rule>

        <rule name="SINGLE_MD5_FILE_EXISTS" level="severe">
            <description>Musí existovat nanejvýše jeden soubor MD5.</description>
            <validationFunction name="CHECK_LIST_EXACT_SIZE">
                <params>
                    <param name="list">
                        <variable name="MD5_FILES"/>
                    </param>
                    <param name="size">1</param>
                </params>
            </validationFunction>
        </rule>


        <!--názvy top-level souborů obsahují povolené znaky-->

        <rule name="INFO_FILE_FILENAME_VALID" level="severe">
            <description>Název souboru INFO obsahuje jen povolené znaky.</description>
            <validationFunction name="CHECK_FILENAME_MATCHES_PATTERN">
                <params>
                    <param name="file">
                        <variable name="INFO_FILE"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MAIN_METS_FILE_FILENAME_VALID" level="severe">
            <description>Název souboru HLAVNI-METS obsahuje jen povolené znaky.</description>
            <validationFunction name="CHECK_FILENAME_MATCHES_PATTERN">
                <params>
                    <param name="file">
                        <variable name="MAIN_METS_FILE"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MD5_FILE_FILENAME_VALID" level="severe">
            <description>Název souboru MD5 obsahuje jen povolené znaky.</description>
            <validationFunction name="CHECK_FILENAME_MATCHES_PATTERN">
                <params>
                    <param name="file">
                        <variable name="MD5_FILE"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--top-level soubory nejsou adresáře-->

        <rule name="INFO_FILE_IS_NOT_DIR" level="severe">
            <description>Soubor INFO není adresář, ale obyčejný soubor.</description>
            <validationFunction name="CHECK_FILE_IS_NOT_DIR">
                <params>
                    <param name="file">
                        <variable name="INFO_FILE"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MAIN_METS_FILE_IS_NOT_DIR" level="severe">
            <description>Soubor HLAVNI-METS není adresář, ale obyčejný soubor.</description>
            <validationFunction name="CHECK_FILE_IS_NOT_DIR">
                <params>
                    <param name="file">
                        <variable name="MAIN_METS_FILE"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MD5_FILE_IS_NOT_DIR" level="severe">
            <description>Soubor MD5 není adresář, ale obyčejný soubor.</description>
            <validationFunction name="CHECK_FILE_IS_NOT_DIR">
                <params>
                    <param name="file">
                        <variable name="MD5_FILE"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--žádné top-level soubory/adresáře kromě očekávaných a tolerovaných-->

        <rule name="TOLERATED_TL_FILES_ARE_NOT_DIRS" level="severe">
            <description>Žádný tolerovaný soubor v kořenovém adresáři nesmí být adresář.</description>
            <validationFunction name="CHECK_NO_FILE_IS_DIR">
                <params>
                    <param name="file_list">
                        <variable name="TOLERATED_TL_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="NO_TOLERATED_TL_FILES_EXIST" level="warning">
            <description>Neměl by existovat žádný tolerovaný soubor.</description>
            <validationFunction name="CHECK_LIST_EMPTY">
                <params>
                    <param name="list">
                        <variable name="TOLERATED_TL_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="NO_OTHER_TOP_LEVEL_FILES_EXIST" level="severe">
            <description>Krome definovaných top-level souborů a adresářů nesmí v kořenovém adresáři existovat žádný jiný
                soubor nebo adresář.
            </description>
            <validationFunction name="CHECK_NO_OTHER_FILES_IN_DIR">
                <params>
                    <param name="root_dir">
                        <variable name="PSP_DIR"/>
                    </param>
                    <param name="file">
                        <variable name="INFO_FILE"/>
                    </param>
                    <param name="file">
                        <variable name="MAIN_METS_FILE"/>
                    </param>
                    <param name="file">
                        <variable name="MD5_FILE"/>
                    </param>
                    <param name="file_list">
                        <variable name="TOLERATED_TL_FILES"/>
                    </param>
                    <param name="file">
                        <variable name="SEC_METS_DIR"/>
                    </param>
                    <param name="file">
                        <variable name="OCR_TEXT_DIR"/>
                    </param>
                    <param name="file">
                        <variable name="OCR_ALTO_DIR"/>
                    </param>
                    <param name="file">
                        <variable name="MC_DIR"/>
                    </param>
                    <param name="file">
                        <variable name="UC_DIR"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--právě jeden každý top-level adresář-->


        <rule name="SINGLE_SEC_METS_DIR_EXISTS" level="severe">
            <description>Musí existovat nanejvýše jeden soubor (adresář) se sekundarními mets soubory.</description>
            <validationFunction name="CHECK_LIST_EXACT_SIZE">
                <params>
                    <param name="list">
                        <variable name="SEC_METS_DIRS"/>
                    </param>
                    <param name="size">1</param>
                </params>
            </validationFunction>
        </rule>

        <rule name="SINGLE_OCR_TEXT_DIR_EXISTS" level="severe">
            <description>Musí existovat nanejvýše jeden soubor (adresář) s textovými ocr soubory.</description>
            <validationFunction name="CHECK_LIST_EXACT_SIZE">
                <params>
                    <param name="list">
                        <variable name="OCR_TEXT_DIRS"/>
                    </param>
                    <param name="size">1</param>
                </params>
            </validationFunction>
        </rule>

        <rule name="SINGLE_OCR_ALTO_DIR_EXISTS" level="severe">
            <description>Musí existovat nanejvýše jeden soubor (adresář) s alto ocr soubory.</description>
            <validationFunction name="CHECK_LIST_EXACT_SIZE">
                <params>
                    <param name="list">
                        <variable name="OCR_ALTO_DIRS"/>
                    </param>
                    <param name="size">1</param>
                </params>
            </validationFunction>
        </rule>

        <rule name="SINGLE_MC_DIR_EXISTS" level="severe">
            <description>Musí existovat nanejvýše jeden soubor (adresář) s master-copy obrazovými soubory.
            </description>
            <validationFunction name="CHECK_LIST_EXACT_SIZE">
                <params>
                    <param name="list">
                        <variable name="MC_DIRS"/>
                    </param>
                    <param name="size">1</param>
                </params>
            </validationFunction>
        </rule>

        <rule name="SINGLE_UC_DIR_EXISTS" level="severe">
            <description>Musí existovat nanejvýše jeden soubor (adresář) s user-copy obrazovými soubory.
            </description>
            <validationFunction name="CHECK_LIST_EXACT_SIZE">
                <params>
                    <param name="list">
                        <variable name="UC_DIRS"/>
                    </param>
                    <param name="size">1</param>
                </params>
            </validationFunction>
        </rule>


        <!--názvy top-level adresářů obsahují povolené znaky-->

        <rule name="SEC_METS_DIR_FILENAME_VALID" level="severe">
            <description>Název adresáře se sekundárními mets soubory musí obsahovat jen povolené znaky.</description>
            <validationFunction name="CHECK_FILENAME_MATCHES_PATTERN">
                <params>
                    <param name="file">
                        <variable name="SEC_METS_DIR"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_TEXT_DIR_FILENAME_VALID" level="severe">
            <description>Název adresáře s textovými ocr soubory musí obsahovat jen povolené znaky.</description>
            <validationFunction name="CHECK_FILENAME_MATCHES_PATTERN">
                <params>
                    <param name="file">
                        <variable name="OCR_TEXT_DIR"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_ALTO_DIR_FILENAME_VALID" level="severe">
            <description>Název adresáře s alto ocr soubory musí obsahovat jen povolené znaky.</description>
            <validationFunction name="CHECK_FILENAME_MATCHES_PATTERN">
                <params>
                    <param name="file">
                        <variable name="OCR_ALTO_DIR"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MC_DIR_FILENAME_VALID" level="severe">
            <description>Název adresáře s master-copy obrazovými soubory musí obsahovat jen povolené znaky.
            </description>
            <validationFunction name="CHECK_FILENAME_MATCHES_PATTERN">
                <params>
                    <param name="file">
                        <variable name="MC_DIR"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="UC_DIR_FILENAME_VALID" level="severe">
            <description>Název adresáře s user-copy obrazovými soubory musí obsahovat jen povolené znaky.
            </description>
            <validationFunction name="CHECK_FILENAME_MATCHES_PATTERN">
                <params>
                    <param name="file">
                        <variable name="UC_DIR"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--top-level adresáře jsou adresáře (ne obyčejné soubory)-->

        <rule name="SEC_METS_DIR_IS_DIR" level="severe">
            <description>Adresář se sekundárními mets soubory musí být opravdu adresář a ne obyčejný soubor
            </description>
            <validationFunction name="CHECK_FILE_IS_DIR">
                <params>
                    <param name="file">
                        <variable name="SEC_METS_DIR"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_TEXT_DIR_IS_DIR" level="severe">
            <description>Adresář s textovýmí ocr soubory musí být opravdu adresář a ne obyčejný soubor
            </description>
            <validationFunction name="CHECK_FILE_IS_DIR">
                <params>
                    <param name="file">
                        <variable name="OCR_TEXT_DIR"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_ALTO_DIR_IS_DIR" level="severe">
            <description>Adresář s alto ocr soubory musí být opravdu adresář a ne obyčejný soubor
            </description>
            <validationFunction name="CHECK_FILE_IS_DIR">
                <params>
                    <param name="file">
                        <variable name="OCR_ALTO_DIR"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MC_DIR_IS_DIR" level="severe">
            <description>Adresář s master-copy obrazovými soubory musí být opravdu adresář a ne obyčejný soubor
            </description>
            <validationFunction name="CHECK_FILE_IS_DIR">
                <params>
                    <param name="file">
                        <variable name="MC_DIR"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="UC_DIR_IS_DIR" level="severe">
            <description>Adresář s user-copy obrazovými soubory musí být opravdu adresář a ne obyčejný soubor
            </description>
            <validationFunction name="CHECK_FILE_IS_DIR">
                <params>
                    <param name="file">
                        <variable name="UC_DIR"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--názvy souborů v top-level adresářích obsahují jen povolené znaky-->


        <rule name="SEC_METS_FILENAMES_VALID" level="severe">
            <description>Všechny sekundárními mets soubory musí obsahovat jen povolené znaky.</description>
            <validationFunction name="CHECK_ALL_FILENAMES_MATCH_PATTERN">
                <params>
                    <param name="file_list">
                        <variable name="SEC_METS_FILES"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_TEXT_FILENAMES_VALID" level="severe">
            <description>Všechny textové ocr soubory musí obsahovat jen povolené znaky.</description>
            <validationFunction name="CHECK_ALL_FILENAMES_MATCH_PATTERN">
                <params>
                    <param name="file_list">
                        <variable name="OCR_TEXT_FILES"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_ALTO_FILENAMES_VALID" level="severe">
            <description>Všechny alto ocr soubory musí obsahovat jen povolené znaky.</description>
            <validationFunction name="CHECK_ALL_FILENAMES_MATCH_PATTERN">
                <params>
                    <param name="file_list">
                        <variable name="OCR_ALTO_FILES"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MC_FILENAMES_VALID" level="severe">
            <description>Všechny master-copy obrazové soubory musí obsahovat jen povolené znaky.</description>
            <validationFunction name="CHECK_ALL_FILENAMES_MATCH_PATTERN">
                <params>
                    <param name="file_list">
                        <variable name="MC_FILES"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="UC_FILENAMES_VALID" level="severe">
            <description>Všechny user-copy obrazové soubory musí obsahovat jen povolené znaky.</description>
            <validationFunction name="CHECK_ALL_FILENAMES_MATCH_PATTERN">
                <params>
                    <param name="file_list">
                        <variable name="UC_FILES"/>
                    </param>
                    <param name="pattern">
                        <pattern name="FILENAME_CHARS_PATTERN"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--počty souborů a délky názvů soborů v top-level adesářích-->

        <rule name="NUMBER_OF_FILES_IN_DIRS_MATCH" level="severe">
            <description>Počet souborů ve všech adresářích se musí shodovat.</description>
            <validationFunction name="CHECK_ALL_LISTS_SAME_SIZE">
                <params>
                    <param name="list">
                        <variable name="SEC_METS_FILES"/>
                    </param>
                    <param name="list">
                        <variable name="OCR_TEXT_FILES"/>
                    </param>
                    <param name="list">
                        <variable name="OCR_ALTO_FILES"/>
                    </param>
                    <param name="list">
                        <variable name="MC_FILES"/>
                    </param>
                    <param name="list">
                        <variable name="UC_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="SEC_METS_FILENAMES_LENGTHS_SAME" level="severe">
            <description>Délka názvů všech sekundárních mets souborů musí být stejná. Tj. čísla všech stran jsou
                zarovnaná
                stejně.
            </description>
            <validationFunction name="CHECK_FILENAMES_LENGTHS_SAME">
                <params>
                    <param name="file_list">
                        <variable name="SEC_METS_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_TEXT_FILENAMES_LENGTHS_SAME" level="severe">
            <description>Délka názvů všech textových ocr souborů musí být stejná. Tj. čísla všech stran jsou zarovnaná
                stejně.
            </description>
            <validationFunction name="CHECK_FILENAMES_LENGTHS_SAME">
                <params>
                    <param name="file_list">
                        <variable name="OCR_TEXT_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_ALTO_FILENAMES_LENGTHS_SAME" level="severe">
            <description>Délka názvů všech alto ocr souborů musí být stejná. Tj. čísla všech stran jsou zarovnaná
                stejně.
            </description>
            <validationFunction name="CHECK_FILENAMES_LENGTHS_SAME">
                <params>
                    <param name="file_list">
                        <variable name="OCR_ALTO_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MC_FILENAMES_LENGTHS_SAME" level="severe">
            <description>Délka názvů všech master-copy obrazových souborů musí být stejná. Tj. čísla všech stran jsou
                zarovnaná
                stejně.
            </description>
            <validationFunction name="CHECK_FILENAMES_LENGTHS_SAME">
                <params>
                    <param name="file_list">
                        <variable name="MC_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="UC_FILENAMES_LENGTHS_SAME" level="severe">
            <description>Délka názvů všech user-copy obrazových souborů musí být stejná. Tj. čísla všech stran jsou
                zarovnaná
                stejně.
            </description>
            <validationFunction name="CHECK_FILENAMES_LENGTHS_SAME">
                <params>
                    <param name="file_list">
                        <variable name="UC_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--žádný ze souborů v top-level adresářích není adresář-->

        <rule name="SEC_METS_FILES_ARE_NOT_DIRS" level="severe">
            <description>Žádný sekundární mets soubor nesmí být adresář.</description>
            <validationFunction name="CHECK_NO_FILE_IS_DIR">
                <params>
                    <param name="file_list">
                        <variable name="SEC_METS_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_TEXT_FILES_ARE_NOT_DIRS" level="severe">
            <description>Žádný textový ocr soubor nesmí být adresář.</description>
            <validationFunction name="CHECK_NO_FILE_IS_DIR">
                <params>
                    <param name="file_list">
                        <variable name="OCR_TEXT_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="OCR_ALTO_FILES_ARE_NOT_DIRS" level="severe">
            <description>Žádný alto ocr soubor nesmí být adresář.</description>
            <validationFunction name="CHECK_NO_FILE_IS_DIR">
                <params>
                    <param name="file_list">
                        <variable name="OCR_ALTO_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="MC_FILES_ARE_NOT_DIRS" level="severe">
            <description>Žádný master-copy obrazový soubor nesmí být adresář.</description>
            <validationFunction name="CHECK_NO_FILE_IS_DIR">
                <params>
                    <param name="file_list">
                        <variable name="MC_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="UC_FILES_ARE_NOT_DIRS" level="severe">
            <description>Žádný user-copy obrazový soubor nesmí být adresář.</description>
            <validationFunction name="CHECK_NO_FILE_IS_DIR">
                <params>
                    <param name="file_list">
                        <variable name="UC_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>


        <!--žádné další soubory v top-level adresářích-->


        <rule name="NO_EXTRA_SEC_METS_FILES" level="severe">
            <description>Kromě sekundárních mets souborů nesmí adresář obsahovat žádné další soubory.</description>
            <validationFunction name="CHECK_NO_OTHER_FILES_IN_DIR">
                <params>
                    <param name="root_dir">
                        <variable name="SEC_METS_DIR"/>
                    </param>
                    <param name="file_list">
                        <variable name="SEC_METS_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="NO_EXTRA_OCR_TEXT_FILES" level="severe">
            <description>Kromě textových ocr souborů nesmí adresář obsahovat žádné další soubory.</description>
            <validationFunction name="CHECK_NO_OTHER_FILES_IN_DIR">
                <params>
                    <param name="root_dir">
                        <variable name="OCR_TEXT_DIR"/>
                    </param>
                    <param name="file_list">
                        <variable name="OCR_TEXT_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="NO_EXTRA_OCR_ALTO_FILES" level="severe">
            <description>Kromě alto ocr souborů nesmí adresář obsahovat žádné další soubory.</description>
            <validationFunction name="CHECK_NO_OTHER_FILES_IN_DIR">
                <params>
                    <param name="root_dir">
                        <variable name="OCR_ALTO_DIR"/>
                    </param>
                    <param name="file_list">
                        <variable name="OCR_ALTO_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="NO_EXTRA_MC_FILES" level="severe">
            <description>Kromě master-copy obrazových souborů nesmí adresář obsahovat žádné další soubory.</description>
            <validationFunction name="CHECK_NO_OTHER_FILES_IN_DIR">
                <params>
                    <param name="root_dir">
                        <variable name="MC_DIR"/>
                    </param>
                    <param name="file_list">
                        <variable name="MC_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

        <rule name="NO_EXTRA_UC_FILES" level="severe">
            <description>Kromě user-copy obrazových souborů nesmí adresář obsahovat žádné další soubory.</description>
            <validationFunction name="CHECK_NO_OTHER_FILES_IN_DIR">
                <params>
                    <param name="root_dir">
                        <variable name="UC_DIR"/>
                    </param>
                    <param name="file_list">
                        <variable name="UC_FILES"/>
                    </param>
                </params>
            </validationFunction>
        </rule>

    </section>

